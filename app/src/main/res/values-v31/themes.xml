<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.AiLabApp" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:windowBackground">@color/primary_blue</item>
        <item name="android:statusBarColor">@color/primary_blue</item>
        <item name="android:navigationBarColor">@color/primary_blue</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/primary_blue</item>
        <item name="windowSplashScreenAnimatedIcon">@android:color/transparent</item>
        <item name="postSplashScreenTheme">@style/Theme.AiLabApp</item>
    </style>
</resources>
